{
  "name": "msg-2-eml",
  "version": "1.0.0",
  "description": "Convert MSG files to EML format",
  "type": "module",
  "scripts": {
    "build:server": "tsc -p tsconfig.json",
    "build:client": "tsc -p src/client/tsconfig.json && cp src/client/index.html dist/client/",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/server/index.js",
    "dev": "npm run build && npm start",
    "test": "npm run build:server && node --test dist/server/*.test.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "msg-parser": "^1.0.10",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
